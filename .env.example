# Secret key used for cookie validation.
COOKIE_VALIDATION_KEY="PH2QMquwNHmWZM0Crhloj9Bmq8K9oKSK"

# Whether to save email messages as files instead of sending
# them to the actual recipients.
MAILER_FILE_TRANSPORT="true"
