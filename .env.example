# Specifies which environment the application is running in.
YII_ENV="dev"

# Specifies whether the application is running in debug mode.
YII_DEBUG="true"

# Information required to connect to the database.
DB_DSN="mysql:host=localhost;dbname=dbname"

# User name for the DSN string.
